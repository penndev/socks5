<resources>
    <item name="ic_launcher" type="mipmap">@mipmap/ic_launcher</item>

    <style name="Socks5BottomNavigationView">
        <item name="itemIconTint">@color/nav_item_color</item>
        <item name="itemTextColor">@color/nav_item_color</item>
    </style>
    <style name="Socks5NodeInput">
        <item name="android:background">@drawable/radius_8dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">20dp</item>
    </style>

</resources>

